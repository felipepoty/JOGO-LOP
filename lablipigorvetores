#include <iostream>
using namespace std;
int main(){
    int n,m,cont=0,cont2=0;
    bool imprimir;
    cin>>n;
    int seq1[n];
    for(int i=0;i<n;i++){
        cin>>seq1[i];
    }
    cin>>m;
    int seq2[m];
    for(int j=0;j<m;j++){
        cin>>seq2[j];
    }
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            if(seq1[i]==seq2[i+j]){
                cont++;
                if(cont==n){
                cont2++;
            }
            }
            }

        }


        cout<<"a sequencia se repetiu "<<cont2<<" vezes"<<endl;


}
